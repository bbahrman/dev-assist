  "homepage": "https://github.com/bbahrman/dev-assist#readme",
  "dependencies": {
    "eslint": "^4.15.0",
    "nodegit": "^0.20.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0"
  }
}
